{
    "name": "axel-f",
    "version": "1.0.0",
    "description": "axel-f is an engine that can evaluate excel-like expressions.",
    "homepage": "https://github.com/xapix-io/axel-f",
    "author": "Kirill Chernyshov (https://github.com/DeLaGuardo)",
    "keywords": [
        "excel",
        "expression",
        "language",
        "compiler",
        "lexer",
        "clojure",
        "clojurescript"
    ],
    "contributors": [
        "Kirill Chernyshov (https://github.com/DeLaGuardo)",
        "Anatoly Smolyaninov (https://github.com/zarkone)",
        "xapix.io (https://github.com/xapix-io)"
    ],
    "bugs": {
        "url": "https://github.com/xapix-io/axel-f/issues"
    },
    "license": "EPL-1.0",
    "main": "./index.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/xapix-io/axel-f.git"
    },
    "engines": {
        "node": ">=0.8.28"
    },
    "dependencies": {
        "@js-joda/core": "1.12.0",
        "@js-joda/locale_en-us": "3.1.1",
        "@js-joda/timezone": "2.2.0"
    },
    "scripts": {
        "clean": "rm -rf ./release-js/lib ./test-js",
        "dev": "npm install && ./node_modules/.bin/shadow-cljs -A:cljs watch package",
        "build": "npm run clean && npm install && ./node_modules/.bin/shadow-cljs -A:cljs release package",
        "ci-tests": "npm run clean && npm install && ./node_modules/.bin/shadow-cljs -A:cljs:kaocha compile ci-tests && ./node_modules/.bin/karma start --single-run"
    },
    "devDependencies": {
        "karma": "^5.0.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-cljs-test": "^0.1.0",
        "shadow-cljs": "^2.8.109"
    }
}
